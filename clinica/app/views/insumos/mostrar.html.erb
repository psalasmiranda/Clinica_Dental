

<div class="panel panel-default" >
  <a class="list-group-item active">
    <%= @insumo.nombre %>
  </a>
  <div class="panel-footer">Cantidad: <%= @insumo.cantidad %><br></div>
  <div class="panel-footer">Costo : $ <%= @insumo.costo %><br></div>
  <div class="panel-footer">Fecha de Ingreso: <% a = @insumo.ingreso %>
  <%=a.to_formatted_s(:long)%></div>
  <div class="panel-footer">Fecha de Vencimiento: <%  b= @insumo.vencimiento %>
  <%=b.to_formatted_s(:long)%></div>
</div>
<%= link_to 'volver', '/insumos' %>
